<%= form_for @appointment do |f| %>
    <%= f.label :patient_id %>
    <br>
    <%= f.number_field :patient_id %>
    <br>
    <%= f.label :doctor_id %>
    <br>
    <%= f.number_field :doctor_id %>
    <br>
    <%= f.label :condition %>
    <br>
    <%= f.text_field :condition %>
    <br>
    <%= f.label :date %>
    <br>
    <%= f.date_field :date %>
    <br>
    <%= f.submit %>
<% end %> 